    <member name="M:System.IO.Pipelines.PipeWriter.AsStream(System.Boolean)">
      <summary>Returns a <see cref="T:System.IO.Stream" /> representation of the <see cref="T:System.IO.Pipelines.PipeWriter" />.</summary>
